{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "sourceMap": true,
    "outDir": "./public",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "declaration": false,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "exclude": ["public", "node_modules"],
  "include": ["./**/*.tsx", "src/types/**/*.d.ts"]
}
